{
    "presets": [
        "react",
        "es2015",
        "latest",
        "stage-3"
    ],
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": false,
                "ployfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ],
        [
            "transform-class-properties"
        ],
        ["import", {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": "css" // `style: true` 会加载 less 文件
        }]
    ]
}